<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/charginglevel"
        android:layout_width="186dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.272"
        tools:text="b is" />

    <ImageView
        android:id="@+id/isCharge"
        android:layout_width="128dp"
        android:layout_height="114dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/charginglevel"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_charging_battery_svgrepo_com" />

    <ImageView
        android:id="@+id/iconBColor"
        android:layout_width="143dp"
        android:layout_height="84dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/charginglevel"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_battery_svgrepo_com__1_" />


</androidx.constraintlayout.widget.ConstraintLayout>